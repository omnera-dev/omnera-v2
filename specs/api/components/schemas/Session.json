{
  "type": "object",
  "description": "User session information",
  "required": ["id", "expiresAt", "token", "createdAt", "updatedAt", "userId"],
  "properties": {
    "id": {
      "type": "string",
      "description": "Unique session identifier",
      "example": "sess_abc123xyz"
    },
    "expiresAt": {
      "type": "string",
      "format": "date-time",
      "description": "Timestamp when the session expires",
      "example": "2025-01-22T10:30:00Z"
    },
    "token": {
      "type": "string",
      "description": "Session token used for authentication",
      "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..."
    },
    "createdAt": {
      "type": "string",
      "format": "date-time",
      "description": "Timestamp when the session was created",
      "example": "2025-01-15T10:30:00Z"
    },
    "updatedAt": {
      "type": "string",
      "format": "date-time",
      "description": "Timestamp when the session was last updated",
      "example": "2025-01-15T10:30:00Z"
    },
    "ipAddress": {
      "type": "string",
      "description": "IP address where the session was created",
      "nullable": true,
      "example": "192.168.1.1"
    },
    "userAgent": {
      "type": "string",
      "description": "User agent string of the client that created the session",
      "nullable": true,
      "example": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36"
    },
    "userId": {
      "type": "string",
      "description": "ID of the user this session belongs to",
      "example": "user_123abc"
    }
  }
}
