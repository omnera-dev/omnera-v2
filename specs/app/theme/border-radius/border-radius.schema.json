{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "border-radius.schema.json",
  "title": "Border Radius",
  "description": "Border radius design tokens",
  "type": "object",
  "patternProperties": {
    "^[a-z]+(-[a-z]+)*$": {
      "type": "string",
      "description": "CSS border-radius value"
    }
  },
  "additionalProperties": false,
  "examples": [
    {
      "none": "0",
      "sm": "0.125rem",
      "md": "0.375rem",
      "lg": "0.5rem",
      "xl": "0.75rem",
      "2xl": "1rem",
      "3xl": "1.5rem",
      "full": "9999px"
    }
  ],
  "specs": []
}
