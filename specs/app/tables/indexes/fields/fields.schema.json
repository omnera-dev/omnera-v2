{
  "$id": "fields.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Index Fields",
  "description": "Field names to include in the index. Order matters for multi-column indexes.",
  "type": "array",
  "items": {
    "type": "string",
    "minLength": 1
  },
  "minItems": 1,
  "examples": [
    [
      "email"
    ],
    [
      "first_name",
      "last_name"
    ],
    [
      "status",
      "created_at"
    ]
  ],
  "specs": []
}
