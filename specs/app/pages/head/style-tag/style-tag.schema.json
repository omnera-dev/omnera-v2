{
  "$id": "style-tag.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Style Tag",
  "description": "HTML style element for inline CSS",
  "type": "object",
  "properties": {
    "tag": {
      "const": "style"
    },
    "content": {
      "type": "string",
      "minLength": 1
    }
  },
  "required": ["tag", "content"],
  "additionalProperties": false
}
